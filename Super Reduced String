  static String superReducedString(String s) {
     StringBuffer str = new StringBuffer(s);
        for(int i=1;i<str.length();i++){
            if(str.charAt(i)==str.charAt(i-1)){
                str.delete(i-1,i+1);
                i=0;

        }
       
    }
     if(str.length()==0){return("Empty String");
     }
    else{
     String r=str.toString();
     return r;
    }
}
     
