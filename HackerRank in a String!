static String hackerrankInString(String s) {
        String r="hackerrank";
         if (s.length() < r.length()) {
            return "NO";
        }
        int j=0;
        for(int i=0;i<s.length();i++){
            if(j<r.length() && s.charAt(i)==r.charAt(j)){
                j++;
            }
        }
        return (j==r.length()?"YES":"NO");
    }
